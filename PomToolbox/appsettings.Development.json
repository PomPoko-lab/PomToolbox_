{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5089"
      },
      "Https": {
        "Url": "https://*:5090",
        "Certificate": {
          "Path": "./certs/temp_cert.pem",
          "KeyPath": "./certs/temp_cert.key"
        }
      }
    }
  }
}
